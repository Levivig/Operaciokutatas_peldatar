proc optmodel;
 	number n;
    var x{1..n} >= 0 integer;
 	n = 4;   
	
	con J1:  x[1] <= 140,
		J2:  x[2] <= 106,
		J3:  x[3] <= 92,
      	J4:  x[4] <= 112,
        O1:  x[1] + x[2] + x[3] + x[4] <= 350;

	MAX z = 340*x[1] + 440*x[2] + 380*x[3] + 380*x[4];

	solve;

	print x;

run;

