proc optmodel;
 	number n;
    var x{1..n} >= 0 integer;
 	n = 3;   
	
	con Varras:   72*x[1] +  56*x[2] +  40*x[3] <= 22*40*60,
		Mintazas: 33*x[1] +  62*x[2] +  27*x[3] <= 13*40*60,
		Vasalas:  30*x[1] +  32*x[2] +  21*x[3] <= 5*40*60,
      	Anyag:   350*x[1] + 150*x[2] + 100*x[3] <= 21000,
        Cerna:     3*x[1] +   6*x[2] +   4*x[3] <= 350;

	MAX z = 700*x[1] + 900*x[2] + 550*x[3];

	solve;

    print x;

run;

