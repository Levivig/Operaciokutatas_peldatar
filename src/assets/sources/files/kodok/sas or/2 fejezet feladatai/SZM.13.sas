proc optmodel;
 	number n;
    var x{1..n} >= 0 integer;
 	n = 6;   
	
	con A: 20*x[1] + 10*x[2] + 30*x[3] + 10*x[4] + 10*x[5] + 20*x[6] >= 100,
		B: 30*x[1] + 20*x[2] + 30*x[3] + 40*x[4] + 10*x[5] + 20*x[6] >= 100,
		C: 10*x[1] + 10*x[2] + 20*x[3] + 40*x[4] + 50*x[5] + 30*x[6] >= 180;

	MIN z = 50*x[1] + 65*x[2] + 40*x[3] + 70*x[4] + 65*x[5] + 60*x[6];

	solve;

    print x;

run;

