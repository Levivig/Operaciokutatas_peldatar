proc optmodel;
 	number n;
    var x{1..n} >= 0 integer;
 	n = 3;   
	
	con G1:     100*x[1] + 60*x[2] + 75*x[3] <= 40*60,
		G2:      80*x[1] + 50*x[2] + 55*x[3] <= 40*60,
		Acel:   1.4*x[1] +2.3*x[2] +2.5*x[3] <= 50,
      	Bor:     50*x[1] + 35*x[2] + 40*x[3] <= 1600,
        Muanyag: 45*x[1] + 40*x[2] + 35*x[3] <= 2000;


	MAX z = 2.1*x[1] + 2.6*x[2] + 3*x[3];

	solve;

	print x;

run;

