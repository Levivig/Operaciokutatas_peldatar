proc optmodel;
 	number n;
    var x{1..n} >= 0 integer;
 	n= 2;   
	
	con FINOM:   6*x[1] + 2*x[2] >= 15,
		NORMAL:  5*x[1] + 2*x[2] >=  9,
		DURVA:   3*x[1] + 7*x[2] >= 20;

	MIN z = 130000*x[1] + 160000*x[2];

	solve;

	print x;

run;

