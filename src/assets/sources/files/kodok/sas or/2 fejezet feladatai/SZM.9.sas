proc optmodel;
 	number n;
    var x{1..n} >= 0 integer;
 	n = 4;   
	
	con Bor:    10*x[1] + 14*x[2]                     <= 250,
		Szovet:  2*x[1] +  3*x[2] + 14*x[3] + 12*x[4] <= 350,
		Fa1:    25*x[1] + 22*x[2] + 19*x[3] + 18*x[4] <= 1000,
		Ora:     3*x[1] +  4*x[2] +  2*x[3] +2.5*x[4] <= 200;

	MAX z = 14000*x[1] + 15500*x[2] + 13000*x[3] + 12500*x[4];

	solve;

    print x;

run;

